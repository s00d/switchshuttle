{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "productName": "switch-shuttle",
  "version": "1.3.1-rc.0",
  "version": "../package.json",
  "identifier": "com.SwitchShuttle.app",
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://raw.githubusercontent.com/s00d/switchshuttle/update/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQ1Q0IzNkFDQkZFMjRDQjkKUldTNVRPSy9yRGJMUlVVSlZYaUlTc1ZkZkFHYnFSb1pnWmNVeTVXdnFKK3BZZHVWamptM0RHT00K",
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Main",
        "width": 400,
        "height": 200,
        "x": 0,
        "y": 0,
        "fullscreen": false,
        "resizable": false,
        "decorations": false,
        "visible": false,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    },
    "trayIcon": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true,
      "id": "switch-shuttle-tray"
    }
  }
}
